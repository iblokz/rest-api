{
  "name": "iblokz-rest-api",
  "description": "Generates a rest api with mongodb middleware from a json file",
  "version": "1.10.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/iblokz/rest-api.git"
  },
  "scripts": {
    "test": "mocha --colors 'test/**/*.test.js'",
    "test-watch": "npm run test -- --watch",
    "coverage": "nym npm run test",
    "lint": "eslint lib",
    "doc": "jsdoc -c jsdoc.json"
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "node_modules/",
      "test/",
      "example/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "author": {
    "name": "Alex Milanov",
    "email": "alex@i4web.biz"
  },
  "license": "MIT",
  "dependencies": {
    "chalk": "^2.3.0",
    "cli-table": "^0.3.1",
    "express": "~4.17.3",
    "http-status": "^1.0.1",
    "iblokz-data": "^1.5.0",
    "mongoose": "^4.7.7",
    "node-http-error": "^2.0.0"
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-config-google": "^0.6.0",
    "faker": "^4.1.0",
    "jsdoc": "^3.6.3",
    "method-override": "^3.0.0",
    "mocha": "^3.2.0",
    "mongodb-memory-server": "^6.6.0",
    "nyc": "^10.1.2",
    "supertest": "^4.0.2",
    "uuid": "^3.3.3"
  }
}
